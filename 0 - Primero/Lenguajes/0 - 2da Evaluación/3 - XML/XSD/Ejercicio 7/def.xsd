<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="musicos">

        <xs:complexType>

            <xs:sequence>

                <xs:element name="musico" minOccurs="0" maxOccurs="unbounded">

                    <xs:complexType>

                        <xs:sequence>

                            <xs:element name="nombre" type="xs:string" minOccurs="0" maxOccurs="1"> </xs:element>

                            <xs:element name="apodo" type="xs:string" minOccurs="0" maxOccurs="2"></xs:element>

                            <xs:element name="fechaNacimiento" type="xs:string"></xs:element>


                        </xs:sequence>

                    </xs:complexType>

                </xs:element>

            </xs:sequence>

        </xs:complexType>

    </xs:element>


</xs:schema>